<div class="container">
  <div class="row">
    <div class="m-4">
      <h4>カテゴリー編集</h4>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12 mt-3">

      <table>
        <%= form_with model: @category, url:  admin_category_path,method: :patch do |f| %>
          <td><%= f.label :"カテゴリー名", class: "mr-3" %></td>
          <td><%= f.text_field :name, class: "mr-5" %></td>
          <td><%= f.submit "変更を保存", class: "btn btn-outline-success" %></td>
        <% end %>
      </table>

    </div>
  </div>
</div>